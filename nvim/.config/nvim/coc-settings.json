{
    "languageserver": {
      "c": {
        "command": "clangd",
        "filetypes": ["c", "cpp", "cc"],
        "initializationOptions": {},
        "settings": {}
      },
      "go": {
        "command": "go-langserver",
        "filetypes": ["go"],
        "initializationOptions": {},
        "settings": {},
        "args": [
          "-gocodecompletion",
          "-freeosmemory=false",
          "-func-snippet-enabled=false"
        ]
      }
  },
  "coc.source.gocode.enable": false,
  "coc.preferences.autoTrigger": "always",
  "coc.preferences.triggerAfterInsertEnter": true,
  "coc.source.ultisnips.enable": true
}
